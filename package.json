{
  "name": "nuxt-craftcms",
  "version": "0.3.0",
  "description": "Nuxt module to use craft cms query builder in nuxt",
  "repository": "https://github.com/samuelreichor/nuxt-craftcms",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint . && npm run test:types",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "ci": "npm run lint && npm run test && npm run prepack",
    "release": "npm run ci && changeset publish",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "vue-craftcms": "0.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@nuxt/devtools": "^1.5.0",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "latest",
    "eslint": "^9.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nuxt": "^3.13.0",
    "typescript": "latest",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "npm run lint:fix"
  }
}
